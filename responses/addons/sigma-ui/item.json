        "status": "stable",
        "installations": 102,
        "rating": "4.5",
        "ratings": [
          { "rating": 5, "comment": "works perfectly" }
        ],
        "install": {
          "bundles": {
            "vendor-lib.js": {                
            },
            "app.js": {  
              "source": [
                "<modules>/sigma-ui/**/*.js"
              ]              
            }
          },
          "dependencies": {
            "libs": [
              "jquery"
            ],
            "plugins": [
              "aurelia-validate",
              "aurelia-i18n"
            ],
            "typescript": {
              "typings": [
                "jquery"
              ],
              "dtsSources": [
                "<modules>/jquery/**/*.d.ts"        
              ]
            }
          }
        }
      }              
    }
  ]
